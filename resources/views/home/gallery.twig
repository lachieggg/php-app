  <div class="carousel-container">
    <div id="myCarousel" class="carousel slide carousel-wrapper" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner">

        <!-- Carousel elements -->
        <div class="item active">
          <img class="carousel-img" id="carousel-img-0"  src="images/sample.png" alt="Pic0">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-1"  src="images/sample.png" alt="Pic1">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-2"  src="images/sample.png" alt="Pic2">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-3"  src="images/sample.png" alt="Pic3">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-4"  src="images/sample.png" alt="Pic4">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-5"  src="images/sample.png" alt="Pic5">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-6"  src="images/sample.png" alt="Pic6">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-7"  src="images/sample.png" alt="Pic7">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-8"  src="images/sample.png" alt="Pic8">
        </div>

        <div class="item">
          <img class="carousel-img" id="carousel-img-9"  src="images/sample.png" alt="Pic9">
        </div>

        <!-- End Carousel elements -->

      </div>

      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left carousel-control"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right carousel-control"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>

	<script type="module" src="js/generated/home.js"></script>
	<script type="module" src="js/generated/gallery.js"></script>

